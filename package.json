{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "tsc": "turbo run tsc",
    "lint:fix": "turbo run lint -- --fix ",
    "mp:check": "manypkg check",
    "clean": "turbo run clean && rm -rf ./node_modules ./pnpm-lock.yaml",
    "format": "prettier --write \"**/*.{js,cjs,mjs,ts,tsx,md,json}\" --ignore-path .gitignore",
    "plop": "NODE_OPTIONS='--import tsx' plop --plopfile=plopfile.ts",
    "ci:version": "pnpm changeset version && pnpm install --no-frozen-lockfile && git add .",
    "ci:publish": "turbo run build && git status && pnpm publish -r"
  },
  "name": "dalukasdev",
  "packageManager": "pnpm@10.4.0",
  "devDependencies": {
    "@changesets/cli": "^2.27.12",
    "@dalukasdev/eslint-config": "workspace:*",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@manypkg/cli": "^0.23.0",
    "eslint": "^9.20.1",
    "plop": "^4.0.1",
    "prettier": "^3.5.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tsx": "^4.19.2",
    "turbo": "^2.4.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
