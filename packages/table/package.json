{
  "name": "@dalukasdev/table",
  "version": "0.0.19",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "lint": "eslint ./src --ext .ts,.tsx",
    "tsc": "tsc --noEmit",
    "clean": "rm -rf node_modules .turbo",
    "build": "tsup src/index.ts --format cjs,esm --dts --external react dayjs",
    "watch": "tsup src/index.ts --format cjs,esm --dts --external react dayjs --watch",
    "prettier:fix": "prettier --write \"src/**/*.ts*\"",
    "prettier:check": "prettier --check \"src/**/*.ts*\""
  },
  "devDependencies": {
    "@dalukasdev/eslint-config": "0.0.6",
    "@dalukasdev/tailwind": "0.0.6",
    "@dalukasdev/ui": "0.0.19",
    "@types/react": "^18.0.22",
    "@types/react-dom": "^18.0.7",
    "eslint": "^8.40.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "dayjs": "^1.11.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
