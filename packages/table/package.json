{
  "name": "@dalukasdev/table",
  "version": "0.1.6",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "tsc": "tsc --noEmit",
    "clean": "rm -rf node_modules .turbo",
    "build": "tsup src/index.ts --format cjs,esm --dts --external react dayjs",
    "watch": "tsup src/index.ts --format cjs,esm --dts --external react dayjs --watch",
    "prettier:fix": "prettier --write \"src/**/*.ts*\"",
    "prettier:check": "prettier --check \"src/**/*.ts*\""
  },
  "devDependencies": {
    "@dalukasdev/eslint-config": "workspace:*",
    "@dalukasdev/tailwind": "workspace:*",
    "@dalukasdev/ui": "workspace:*",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "eslint": "^9.20.1",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "dayjs": "^1.11.13",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
