{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "lint": {},
    "tsc": {},
    "watch": {
      "outputs": ["dist/**"]
    },
    "clean": {
      "outputs": []
    },
    "prettier:fix": {
      "outputs": []
    },
    "prettier:check": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
